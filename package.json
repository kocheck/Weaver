{
  "name": "weaver-sketch-plugin",
  "version": "1.0.0",
  "description": "Weave realistic data into your Sketch designs using local AI - powered by Ollama, works offline",
  "main": "src/my-command.js",
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js __tests__/**/*.js",
    "lint:fix": "eslint src/**/*.js __tests__/**/*.js --fix",
    "lint:html": "htmlhint src/**/*.html",
    "link": "skpm-link",
    "unlink": "skpm-link --remove",
    "publish": "skpm publish"
  },
  "engines": {
    "sketch": ">=70.0"
  },
  "skpm": {
    "name": "Weaver",
    "manifest": "manifest.json",
    "main": "weaver.sketchplugin",
    "assets": [
      "src/ui.html"
    ]
  },
  "dependencies": {
    "sketch-module-web-view": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@skpm/builder": "^0.7.11",
    "@skpm/test-runner": "^0.4.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "htmlhint": "^1.1.4",
    "jest": "^29.7.0",
    "skpm": "^1.3.3"
  },
  "author": "Weaver Team",
  "license": "MIT",
  "keywords": [
    "sketch",
    "plugin",
    "mock-data",
    "llm",
    "ollama",
    "ai",
    "local-first",
    "offline",
    "privacy"
  ]
}
